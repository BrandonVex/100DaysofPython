print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡
both_names = name1 + name2
lower_names = both_names.lower()
t = lower_names.count("t")
r = lower_names.count("r")
u = lower_names.count("u")
e = lower_names.count("e")
first_digit = t + r + u + e

l = lower_names.count("l")
o = lower_names.count("o")
v = lower_names.count("v")
e = lower_names.count("e")
second_digit = l + o + v + e

final_math = int(str(first_digit) + str(second_digit))

if (final_math < 10) or (final_math > 90):
  print(f"Your score is {final_math}, you go together like coke and mentos.")
elif (final_math >= 40) and (final_math <= 50):
  print(f"Your score is {final_math}, you are alright together.")
else:
  print(f"Your score is {final_math}.")
