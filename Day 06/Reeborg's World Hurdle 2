def turn_right():
    turn_left()
    turn_left()
    turn_left() 
    
def jump():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
    

number_hurdles = 6
while number_hurdles >0:
    jump()
    number_hurdles -= 1
    if at_goal():
        break


